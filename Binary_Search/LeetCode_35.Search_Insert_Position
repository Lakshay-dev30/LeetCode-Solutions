//Brute Force
class Solution {
    public int searchInsert(int[] nums, int target) {
        int n = nums.length;
        int ans = n;
        for(int i=0;i<n;i++){
            if(nums[i] >= target){
                ans = i;
                return ans;
            }
        }
        return n;
    }
}
//T.C = O(n)
//S.C = O(1)
